{
  "mcpServers": {
    "browser-use": {
      "command": "uvx",
      "args": ["browser-use[cli]", "--mcp"],
      "env": {
        "OPENAI_API_KEY": "${OPENAI_API_KEY}",
        "OPENROUTER_API_KEY": "${OPENROUTER_API_KEY}",
        "BROWSER_USE_WEB_UI": "true",
        "BROWSER_USE_WEB_PORT": "8080"
      }
    }
  }
}
